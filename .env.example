# Meraki Dashboard API key (required)
MERAKI_API_KEY=your_meraki_api_key
# Default organization name (optional; required if multiple orgs)
MERAKI_ORG=My Org
# Default network name or ALL
MERAKI_NETWORK=ALL
# Default output format: csv | text | html
OUTPUT_FORMAT=csv
# Override API base URL (optional)
MERAKI_BASE_URL=https://api.meraki.com/api/v1
# Log file path
LOG_FILE=Find-Meraki-Ports-With-MAC.log
# Log level: DEBUG | INFO | WARNING | ERROR
LOG_LEVEL=DEBUG
